<strong>Logging panel</strong>

    <% form_with model: @user do |f| %>

        <p>
            <%= f.label :email %><br>
            <%= f.email_field :email %>
        </p>

        <p>
            <%= f.label :password %>
            <%= f.password_field :password %>
        </p>
    
    <%= f.submit 'Log in' %>

    <%end%>